{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/authorization-server/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/authorization-server/tsconfig.app.json",
    "plugins": [
      "@nestjs/swagger/plugin"
    ]
  },
  "monorepo": true,
  "root": "apps/authorization-server",
  "projects": {
    "authorization-server": {
      "type": "application",
      "root": "apps/authorization-server",
      "entryFile": "main",
      "sourceRoot": "apps/authorization-server/src",
      "compilerOptions": {
        "tsConfigPath": "apps/authorization-server/tsconfig.app.json"
      }
    },
    "auth-api": {
      "type": "application",
      "root": "apps/auth-api",
      "entryFile": "main",
      "sourceRoot": "apps/auth-api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth-api/tsconfig.app.json"
      }
    },
    "local-account-server": {
      "type": "application",
      "root": "apps/local-account-server",
      "entryFile": "main",
      "sourceRoot": "apps/local-account-server/src",
      "compilerOptions": {
        "tsConfigPath": "apps/local-account-server/tsconfig.app.json",
        "assets": [
          "**/*.hbs",
          "**/*.css"
        ]
      }
    },
    "sell-point-core": {
      "type": "application",
      "root": "apps/sell-point-core",
      "entryFile": "main",
      "sourceRoot": "apps/sell-point-core/src",
      "compilerOptions": {
        "tsConfigPath": "apps/sell-point-core/tsconfig.app.json"
      }
    },
    "sell-point-account": {
      "type": "application",
      "root": "apps/sell-point-account",
      "entryFile": "main",
      "sourceRoot": "apps/sell-point-account/src",
      "compilerOptions": {
        "tsConfigPath": "apps/sell-point-account/tsconfig.app.json"
      }
    },
    "sell-point-transaction": {
      "type": "application",
      "root": "apps/sell-point-transaction",
      "entryFile": "main",
      "sourceRoot": "apps/sell-point-transaction/src",
      "compilerOptions": {
        "tsConfigPath": "apps/sell-point-transaction/tsconfig.app.json"
      }
    },
    "sell-point-balance": {
      "type": "application",
      "root": "apps/sell-point-balance",
      "entryFile": "main",
      "sourceRoot": "apps/sell-point-balance/src",
      "compilerOptions": {
        "tsConfigPath": "apps/sell-point-balance/tsconfig.app.json"
      }
    }
  }
}